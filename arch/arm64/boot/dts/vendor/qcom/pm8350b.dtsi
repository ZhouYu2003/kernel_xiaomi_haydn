&thermal_zones {
	pm8350b_temp_alarm: pm8350b_tz {
		polling-delay-passive = <100>;
		polling-delay = <0>;
		thermal-governor = "step_wise";
		thermal-sensors = <&pm8350b_tz>;

		trips {
			pm8350b_trip0: trip0 {
				temperature = <95000>;
				hysteresis = <0>;
				type = "passive";
			};

			pm8350b_trip1: trip1 {
				temperature = <115000>;
				hysteresis = <0>;
				type = "critical";
			};

			pm8350b_trip2: trip2 {
				temperature = <145000>;
				hysteresis = <0>;
				type = "critical";
